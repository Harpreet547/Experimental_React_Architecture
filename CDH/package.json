{
  "name": "@harpreet547/cdh",
  "version": "0.1.0",
  "description": "This project manages Redux, localization, API calls and other services and hooks for react projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
